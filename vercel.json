{
  "version": 2,
  "buildCommand": "pnpm install && pnpm run validate-env && pnpm --filter web build",
  "outputDirectory": "web/dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/((?!api/|assets/|uploads/|.*\\..*).*)",
      "dest": "/index.html"
    }
  ]
}
